{
  "name": "todo-list-monorepo",
  "version": "1.0.0",
  "description": "Todo List Monorepo Project",
  "private": true,
  "workspaces": [
    "front",
    "back"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev:back\" \"npm run dev:front\"",
    "dev:back": "cd back && npm run dev",
    "dev:front": "cd front && npm start",
    "install:all": "npm install && cd back && npm install && cd ../front && npm install"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}

